<!-- nested inside /nexml/trees, preceded by an otus element -->
<!-- A tree with float edges. -->
<tree id="tree1" xsi:type="nex:FloatTree" label="tree1">
  <node id="n1" label="Node 1" root="true"/>  <!-- this is the root node -->
  <node id="n2" label="Node 2" otu="t1"/>  <!-- this is probably a tip -->
  <node id="n3" label="Node 3"/>
  <node id="n4" label="Node 4"/>
  <node id="n5" label="Node 5" otu="t3"/>
